{
  "pipeline": {
    "name": "musictransformer"
  },
  "transform": {
    "image": "hitheory/musictransformer:0.1.9",
    "cmd": [
      "sh",
      "/src/musictransformer.sh"
    ]
  },
  "resource_limits": {
    "memory": "14336M",
    "gpu": {
      "type": "nvidia.com/gpu",
      "number": 1
    }
  },
  "input": {
    "pfs": {
      "repo": "transformer-preprocess",
      "glob": "/"
    }
  }
}