{
  "pipeline": {
    "name": "dev-musictransformer"
  },
  "transform": {
    "image": "hitheory/musictransformer:0.0.5",
    "cmd": [
      "sh",
      "/tf/MusicTransformer-tensorflow2.0/musictransformer.sh",
      "--epochs 22",
      "--max_seq 2048",
      "--pickle_dir /pfs/out/pickle",
      "--save_path /pfs/out/save",
      "--batch_size 2"
    ]
  },
  "resource_limits": {
    "memory": "14336M",
    "gpu": {
      "type": "nvidia.com/gpu",
      "number": 1
    }
  },
  "input": {
    "pfs": {
      "repo": "dev-midi",
      "glob": "/*"
    }
  }
}